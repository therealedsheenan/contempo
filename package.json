  "scripts": {
    "prerun": "rimraf public",
    "build": "webpack --env.dev",
    "build:serve": "webpack --env.prod",
    "prebuild": "concurrently \"npm run prerun\"",
    "prebuild:dev": "concurrently \"npm run prerun\"",
    "build:dev": "webpack --env.dev",
    "dev": "webpack-dev-server --env.development",
    "build:server": "NODE_ENV=production babel-node src/server/index.js",
    "watch": "webpack --watch --progress --colors --watch",
    "server": "concurrently \"npm run build:serve\" \"npm run build:server\"",
    "debug": "webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "test": "NODE_ENV=test NODE_PATH=src/client jest",
    "eslint": "NODE_ENV=test eslint src/client",
    "production": "webpack --env.prod -p",
    "translation": "babel-node src/translation/server.js",
    "watchTranslate": "NODE_ENV=production webpack --progress --colors --watch",
    "translate": "concurrently \"npm run watchTranslate\" \"npm run translation\""
  }

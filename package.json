    "superagent": "^2.3.0"
  },
  "scripts": {
    "prerun": "rimraf public",
    "postrun": "cpy favicon.ico public",
    "postbuild": "concurrently \"npm run postrun\"",
    "build": "webpack --env.prod -p",
    "prebuild": "concurrently \"npm run prerun\"",
    "prebuild:dev": "concurrently \"npm run prerun\"",
    "build:dev": "webpack --env.dev",
    "postbuild:dev": "concurrently \"npm run postrun\"",
    "dev": "webpack-dev-server --env.dev --hot",
    "watch": "webpack --watch --progress --colors --watch",
    "serve": "NODE_ENV=production babel-node src/server/index.js",
    "server": "concurrently \"npm run build\" \"npm run serve\"",
    "debug": "node-nightly --inspect --debug-brk node_modules/.bin/webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "test": "mocha --require test/helpers/setup.js"
  },
